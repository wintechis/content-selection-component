{
  "name": "content-selection-module",
  "version": "1.0.0",
  "author": "smeckler",
  "description": "A minimal example of a repository containing an injectable Community Solid Server module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "lsd:module": "https://linkedsoftwaredependencies.org/bundles/npm/content-selection-module",
  "lsd:components": "dist/components/components.jsonld",
  "lsd:contexts": {
    "https://linkedsoftwaredependencies.org/bundles/npm/content-selection-module/^1.0.0/components/context.jsonld": "dist/components/context.jsonld"
  },
  "lsd:importPaths": {
    "https://linkedsoftwaredependencies.org/bundles/npm/content-selection-module/^1.0.0/components/": "dist/components/",
    "https://linkedsoftwaredependencies.org/bundles/npm/content-selection-module/^1.0.0/config/": "config/",
    "https://linkedsoftwaredependencies.org/bundles/npm/content-selection-module/^1.0.0/dist/": "dist/"
  },
  "scripts": {
    "start": "community-solid-server -c config/contentselectionredirect-file.json -f .data -m .",
    "start-2": "community-solid-server -c config/contentselection-file-override.json -f .data -m .",
    "build": "npm run build:ts && npm run build:components",
    "build:components": "componentsjs-generator -s src -c dist/components -i .componentsignore -r content-selection",
    "build:ts": "tsc",
    "prepare": "npm run build",
    "test": "jest"
  },
  "files": [
    "dist",
    "config"
  ],
  "dependencies": {
    "@solid/community-server": "^7.1.0"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/jest": "^29.5.12",
    "componentsjs-generator": "^3.1.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "license": "DontTouchMyGarbage"
}